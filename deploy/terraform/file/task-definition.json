[
  {
    "essential": true,
    "image": "${image}",
    "memory": 512,
    "memory_reservation": 128,
    "name": "${name}",
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": ${port}
      }
    ],
    "environment": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "prod"
      }
    ]
  }
]
